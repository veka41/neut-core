import {
- this.bool [bool, True, False]
- o [EQ]
- lo [loset, Loset]
}

data entity(a) {
- Entity of {
  - identity: (&a, &a) -> bool // no entity without identity
  }
}

inline from-loset[a](x: loset(a)): entity(a) {
  let Loset of { cmp } = x in
  Entity of {
  - identity =
    (x, y) => {
      match cmp(x, y) {
      - EQ =>
        True
      - _ =>
        False
      }
    }
  }
}
