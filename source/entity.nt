import {
- this.bool {bool, True, False}
- O {EQ}
- Lo {loset, Loset}
}

data entity(a) {
- Entity of {
    // no entity without identity
  - identity: (&a, &a) -> bool
  }
}

inline from-loset<a>(x: loset(a)): entity(a) {
  let Loset of {cmp} = x in
  Entity of {
  - identity =
    (x, y) => {
      match cmp(x, y) {
      - EQ =>
        True
      - _ =>
        False
      }
    }
  }
}
