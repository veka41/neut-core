import {
- this.bool
- this.ordering [GT, LT, EQ]
- e
- ordered [ordered, Ordered]
}

inline as-entity(): e.entity(float) {
  e.from-ordered(as-ordered())
}

inline as-ordered(): ordered(float) {
  Ordered of {
  - identity => eq-float
  - order =>
    (x, y) => {
      if gt-float(x, y) {
        GT
      } else-if lt-float(x, y) {
        LT
      } else {
        EQ
      }
    }
  }
}
