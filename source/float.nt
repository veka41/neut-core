import {
- this.bool
- this.ordering [GT, LT, EQ]
- e
- ordered [ordered, Ordered]
}

inline as-entity(): e.entity(float) {
  e.Entity of {
  - identity => eq-float // no reflexivity due to NaN
  }
}

inline as-ordered(): ordered(float) {
  Ordered of {
  - as-entity => as-entity()
  - order =>
    (x, y) => {
      if gt-float(x, y) {
        GT
      } else-if lt-float(x, y) {
        LT
      } else {
        EQ
      }
    }
  }
}
