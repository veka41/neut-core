import {
- this.bool
- this.field [field, Field]
- this.semiring [semiring, from-field]
- e
- lo [loset, Loset]
- o [GT, LT, EQ]
}

inline as-entity(): e.entity(float) {
  e.from-loset(as-loset())
}

inline as-loset(): loset(float) {
  Loset of {
  - cmp =
    (x, y) => {
      if gt-float(x, y) {
        GT
      } else-if lt-float(x, y) {
        LT
      } else {
        EQ
      }
    }
  }
}

inline as-field(): field(float) {
  Field of {
  - zero = 0
  - add = add-float
  - sub = sub-float
  - one = 1
  - mul = mul-float
  - div = div-float
  }
}

inline as-semiring(): semiring(float) {
  from-field(as-field())
}
