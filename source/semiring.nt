import {
- this.field {Field, field}
- this.ring {Ring, ring}
}

data semiring(a) {
- Semiring of {
  - add: arrow (a, a) -> a
  - mul: arrow (a, a) -> a
  }
}

inline from-field<a>(f: field(a)): semiring(a) {
  let Field of {add, mul} = f in
  Semiring of {add, mul}
}

inline from-ring<a>(f: ring(a)): semiring(a) {
  let Ring of {add, mul} = f in
  Semiring of {add, mul}
}
