import {
- this.field [field, Field]
- this.ring [ring, Ring]
}

data semiring(a) {
- Semiring of {
  - zero: a
  - add: (a, a) -> a
  - one: a
  - mul: (a, a) -> a
  }
}

inline from-field[a](f: field(a)): semiring(a) {
  let Field of { zero, add, one, mul } = f in
  Semiring of { zero, add, one, mul }
}

inline from-ring[a](f: ring(a)): semiring(a) {
  let Ring of { zero, add, one, mul } = f in
  Semiring of { zero, add, one, mul }
}
