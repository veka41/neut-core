import {
  this.list {Cons, append, for-each, intercalate, list},
  this.show {Show, show},
  this.text {join},
}

inline as-show<a>(s: show(a)): show(list(a)) {
  Show(function (xs) {
    let Show(f) = s in
    let ys = for-each(xs, f) in
    join(Cons(*"[", append(intercalate(ys, *", "), [*"]"])))
  })
}
