import {
  this.bool {bool},
  this.entity {Entity, entity},
  this.rune {rune},
}

constant as-entity: entity(rune) {
  Entity of {
    identity = eq-int32,
  }
}

constant as-entity-N: entity(&rune) {
  Entity of {
    identity = {
      function (x, y) {
        as-entity::identity(*x, *y)
      }
    },
  }
}

define eq-rune(r1: rune, r2: rune): bool {
  eq-int32(r1, r2)
}
