import {
  O {EQ, GT, LT},
  this.bool {bool},
  this.box,
  this.ord {Ord, ord},
  this.rune {to-int32},
}

define lt-rune(r1: rune, r2: rune): bool {
  let r1 = to-int32(r1) in
  let r2 = to-int32(r2) in
  lt-int32(r1, r2)
}

define le-rune(r1: rune, r2: rune): bool {
  let r1 = to-int32(r1) in
  let r2 = to-int32(r2) in
  le-int32(r1, r2)
}

define gt-rune(r1: rune, r2: rune): bool {
  let r1 = to-int32(r1) in
  let r2 = to-int32(r2) in
  gt-int32(r1, r2)
}

define ge-rune(r1: rune, r2: rune): bool {
  let r1 = to-int32(r1) in
  let r2 = to-int32(r2) in
  ge-int32(r1, r2)
}

constant as-ord: ord(rune) {
  Ord of {
    compare = {
      function (x, y) {
        if gt-rune(x, y) {
          GT
        } else-if lt-rune(x, y) {
          LT
        } else {
          EQ
        }
      }
    },
  }
}

constant as-ord-N: ord(&rune) {
  Ord of {
    compare = {
      function (x, y) {
        if gt-rune(*x, *y) {
          GT
        } else-if lt-rune(*x, *y) {
          LT
        } else {
          EQ
        }
      }
    },
  }
}
