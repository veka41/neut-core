import {
  this.bool {bool},
  this.box,
  this.show {Show, show},
  this.text {text},
}

constant as-show: show(bool) {
  Show(function (b) {
    if b {
      *"True"
    } else {
      *"False"
    }
  })
}

define show(b: bool): text {
  as-show::show(b)
}

define show-bool(b: bool): text {
  this.bool.show.show(b)
}

define %t(b: bool): text {
  as-show::show(b)
}
