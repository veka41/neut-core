import {
  O {EQ, GT, LT},
  this.bool,
  this.loset {Loset, loset},
}

constant as-loset: loset(int) {
  Loset of {
    cmp = {
      function (x, y) {
        if gt-int(x, y) {
          GT
        } else-if lt-int(x, y) {
          LT
        } else {
          EQ
        }
      }
    },
  }
}

constant as-loset-N: loset(&int) {
  Loset of {
    cmp = {
      function (x, y) {
        if gt-int(*x, *y) {
          GT
        } else-if lt-int(*x, *y) {
          LT
        } else {
          EQ
        }
      }
    },
  }
}
