import {
- this.bool
- this.list [list]
- this.ordering [GT, LT, EQ]
- e
- ordered [ordered, Ordered]
}

inline as-entity(): e.entity(int) {
  e.Entity of {
  - identity => eq-int
  }
}

inline as-ordered(): ordered.ordered(int) {
  Ordered of {
  - as-entity => as-entity()
  - order =>
    (x, y) => {
      if gt-int(x, y) {
        GT
      } else-if lt-int(x, y) {
        LT
      } else {
        EQ
      }
    }
  }
}

define sum-int(xs: list(int)): int {
  let f =
    mu loop(ys: list(int), acc: int): int {
      match ys {
      - [] =>
        acc
      - z :: zs =>
        loop(zs, add-int(z, acc))
      }
    }
  in
  f(xs, 0)
}
