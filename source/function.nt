import {}

export {
- flip
- compose
- curry
- uncurry
}

define flip[a, b, c](f: (a, b) -> c): (b, a) -> c {
  (y, x) => {
    f(x, y)
  }
}

define compose[a, b, c](f: a -> b, g: b -> c): a -> c {
  (x) => {
    g(f(x))
  }
}

define curry[a, b, c](f: (a, b) -> c): a -> b -> c {
  (x) => {
    (y) => {
      f(x, y)
    }
  }
}

define uncurry[a, b, c](f: a -> b -> c): (a, b) -> c {
  (x, y) => {
    f(x)(y)
  }
}
