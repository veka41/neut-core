import {
  this.box,
}

inline flip<a, b, c>(f: (a, b) -> c): (b, a) -> c {
  function (y, x) {
    f(x, y)
  }
}

inline compose<a, b, c>(g: (b) -> c, f: (a) -> b): (a) -> c {
  function (x) {
    g(f(x))
  }
}

inline curry<a, b, c>(f: (a, b) -> c): (a) -> (b) -> c {
  function (x) {
    function (y) {
      f(x, y)
    }
  }
}

inline uncurry<a, b, c>(f: (a) -> (b) -> c): (a, b) -> c {
  function (x, y) {
    f(x)(y)
  }
}

inline lift-N2<a, b, c>(f: (a, b) -> c): (&a, &b) -> c {
  function (x, y) {
    f(*x, *y)
  }
}
