import {
- this.field {Field, field}
- this.ring {Ring, ring}
}

data rig(a) {
- Rig of {
  - zero: a
  - add: arrow (a, a) => a
  - one: a
  - mul: arrow (a, a) => a
  }
}

inline from-field<a>(f: field(a)): rig(a) {
  let Field of {zero, add, one, mul} = f in
  Rig of {zero, add, one, mul}
}

inline from-ring<a>(f: ring(a)): rig(a) {
  let Ring of {zero, add, one, mul} = f in
  Rig of {zero, add, one, mul}
}
