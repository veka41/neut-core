import {
  this.bool {bool},
}

constant _c-pid: type {
  introspect architecture {
  | amd64 =>
    int32
  | arm64 =>
    int32
  }
}

data c-pid {
| C-PID(_c-pid)
}

inline eq-c-pid(x: c-pid, y: c-pid): bool {
  let C-PID(x) = x in
  let C-PID(y) = y in
  introspect architecture {
  | amd64 =>
    eq-int32(x, y)
  | arm64 =>
    eq-int32(x, y)
  }
}
